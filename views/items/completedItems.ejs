
<div class="row">
    <div class="col-md-8 offset-md-2">
        <div class="row">
            <h2>Completed Items</h2>
        </div>
        <table class="table table-hover table-striped table-bordered">
            <tr>
                <th>Title</th>
                <th>Category Name</th>
                <th>Completed Date</th>
            </tr>

            <% for(let i=0;i<items.length;i++){%>
            
                <tr>
                    <td><%=items[i].Title%></td>
                    <td><%=items[i].Name%></td>
                    <td><%=items[i].CompletedDate.toDateString()%></td>
                </tr>
                <% }%>
        </table>
    </div>
</div>